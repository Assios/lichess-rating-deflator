{
  "manifest_version": 3,
  "name": "Lichess Rating Deflator",
  "version": "1.0",
  "description": "This plugin fixes Lichess's rating inflation once and for all.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://lichess.org/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}
